{
  "version": 2,
  "builds": [
    {
      "src": "api/handler.go",
      "use": "@vercel/go",
      "config": {
        "includeFiles": ["infrastructure/**", "application/**", "presentation/**", "domain/**", "config/**", "ogen/**", "middleware/**"]
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "api/handler.go" }]
}
